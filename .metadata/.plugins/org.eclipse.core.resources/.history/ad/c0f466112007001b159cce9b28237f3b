import java.util.Scanner;


public class Main {
	
	public static void main(String[] args) {
		int h1, h2;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Hora de inicio e fim do jogo?");
		h1 = sc.nextInt();
		h2 = sc.nextInt();
		
		if(h1 + h2 < 24 && h1 + h2 > 1) {
			System.out.printf("O JOGO DUROU %d HORA(S)", h1 + h2);
		}
		
		sc.close();
	}
